<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ir_cron_payment_notification" model="ir.cron">
            <field name="name">Project Payment: Send Notifications</field>
            <field name="model_id" ref="model_project_payment"/>
            <field name="state">code</field>
            <field name="code">model._send_payment_notifications()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>